<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration 
	PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" 
	"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

  <context id="context1" defaultModelType="flat">
   <!-- 注释 -->
  	<commentGenerator>
         <property name="suppressDate" value="false"/><!-- 是否取消注释 -->
         <property name="suppressAllComments" value="true"/><!-- 是否生成注释代时间戳-->
    </commentGenerator>
    <!-- JDBC连接 -->
    <jdbcConnection connectionURL="jdbc:mysql://localhost:3306/yjgb_sb?useUnicode=true&amp;characterEncoding=utf8" 
    	driverClass="com.mysql.jdbc.Driver" 
    	userId="root" password="123456"/>
   	<!-- 生成的实体类存放位置 -->
    <javaModelGenerator targetPackage="com.gospell.chitong.rdcenter.broadcast.broadcastMange.entity" targetProject="src/main/java" />
    <!-- 生成的mapper文件存放位置 -->
    <sqlMapGenerator targetPackage="mapper" targetProject="src/main/resources/mybatis" />
    <!-- 生成Dao类存放位置 -->
    <javaClientGenerator targetPackage="com.gospell.chitong.rdcenter.broadcast.broadcastMange.dao" targetProject="src/main/java" type="XMLMAPPER" />
    <table schema="yjgb_sb" tableName="emergencyinfo" domainObjectName="Emergencyinfo" enableCountByExample="false" 
    	enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false"
    	 selectByExampleQueryId="false">
    <!-- 数据库表主键 -->
      <generatedKey column="id" sqlStatement="Mysql"/> 
    </table>
  </context>
</generatorConfiguration>